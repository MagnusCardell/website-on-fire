{
  "name": "website-on-ice",
  "version": "2.1.0",
  "description": "Portfolio webpage and sandbox",
  "main": "dist/server.js",
  "scripts": {
    "start": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "dev": "node .",
    "dev:start": "npm-run-all build dev",
    "build": "npm-run-all clean lint tsc copy-assets",
    "copy-assets": "ts-node tools/copyAssets",
    "tsc": "tsc",
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagnusCardell/website-on-ice.git"
  },
  "author": "Magnus Cardell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MagnusCardell/website-on-ice/issues"
  },
  "homepage": "https://github.com/MagnusCardell/website-on-ice#readme",
  "dependencies": {
    "ejs": "^3.1.8",
    "express": "^4.16.4",
    "tsc-hooks": "^1.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.11.7",
    "@types/shelljs": "^0.8.11",
    "concurrently": "^7.5.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
